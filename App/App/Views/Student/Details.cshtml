@model App.Models.Student
@{
    <h1>Index: <strong> @Model.Index </strong> </h1>
    <p>
        <span class="text-italic">Student:</span> <span class="fw-bold">@Model.Name @Model.Surname <br /></span>
        <span class="text-italic">Data urodzenia:</span> <span class="fw-bold">@Model.DateOfBirth.ToShortDateString() <br /></span>
        <span class="text-italic">Kierunek studiów:</span> <span class="fw-bold">@Model.Direction</span>
    </p>
}
